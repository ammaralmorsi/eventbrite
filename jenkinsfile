            }
       }
    
    
        stage('Test') {
            steps {
                echo "we need to install dependancies first This step tests the project"
                
            }
        }
        
        stage('Deploy') {
            steps {
                echo "This stage deploys the project"
            }
        }        
        stage('Report') {
            steps {
                echo "This stage generates a report"
                
            }
        }
    }
}
